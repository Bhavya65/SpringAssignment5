pipeline {
    tools {
    	jdk 'jdk_1.8.0_221'
  }


    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'Apache Maven 3.6.2') {
                    bat 'mvn clean compile'
                }
            }
        }

        stage ('Testing Stage') {

            steps {
                withMaven(maven : 'Apache Maven 3.6.2') {
                    bat 'mvn test'
                }
            }
        }
    }
}