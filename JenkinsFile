pipeline {
    agent any
    
    tools {
    jdk 'Java development kit 1.8'
  }
    

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'Apache Maven 3.6.2') {
                    bat 'mvn clean compile'
                }
            }
        }

        stage ('Testing Stage') {

            steps {
                withMaven(maven : 'Apache Maven 3.6.2') {
                    bat 'mvn test'
                }
            }
        }
        stage('SonarQube analysis') {
            steps {
            withSonarQubeEnv(credentialsId: 'f225455e-ea59-40fa-8af7-08176e86507a', installationName: 'SonarQube 4.6') { // You can override the credential to be used
      bat 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.7.0.1746:sonar'
      }
    }
  }

    }
}
